<UserControl x:Class="CL_CLegendary_Launcher_.Windows.AddItemModPack"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CL_CLegendary_Launcher_.Windows"
             mc:Ignorable="d" Height="65" Width="177">
    <Grid Background="{DynamicResource MainBackgroundBrushServer}" HorizontalAlignment="Center" Width="177">
        <TextBlock x:Name="NameMod" HorizontalAlignment="Left" Height="50" Margin="45,5,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="90" FontFamily="Inter 18pt 18pt" Padding="0,18,0,0" Foreground="{DynamicResource MainForegroundBrush}"/>
        <Image x:Name="DeleteModFromModPack" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Left" Height="30" Margin="145,15,0,0" VerticalAlignment="Top" Width="30" Source="/Assets/delete_1000dp_E8EAED_FILL0_wght400_GRAD0_opsz48.png"/>
        <Image x:Name="IconMod" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Left" Height="30" Margin="5,15,0,0" VerticalAlignment="Top" Width="30"/>
    </Grid>
</UserControl>
